    <main class="mt-4" style="width: 100%;">
        <h1 class="text-center mt-4">Self Collect Orders (<%= totalRecords %>)</h1>
        <div class="table-responsive">
            <table class="table table-bordered table-striped" id="orderTable">
                <thead class="table-primary">
                    <tr>
                        <th class="header">Tracking No.</th>
                        <th class="header">Product</th>
                        <th class="header">Receiver Name</th>
                        <th class="header">Current Status</th>
                        <th class="header">Assigned To</th>
                        <th class="header">Job Date</th>
                        <th class="header">Attempt No.</th>
                        <th class="header">Last Update</th>
                        <th class="header">Last Update By</th>
                        <th class="header">Job Created</th>
                        <th class="header">Phone No.</th>
                        <th class="header">Additional Phone No.</th>
                        <th class="header">Address</th>
                        <th class="header">Area</th>
                        <th class="header">Item Description</th>
                        <th class="header">Payment Method</th>
                        <th class="header">Price</th>
                        <th class="header">Remarks</th>
                    </tr>
                </thead>

                <tbody>
                    <!-- Iterate through the orders and display the data -->
                    <% orders.forEach(function(order) { %>
                        <tr>
                            <td>
                                <%= order.doTrackingNumber %>
                            </td>
                            <td>
                                <%= order.product %>
                            </td>
                            <td>
                                <%= order.receiverName %>
                            </td>
                            <td>
                                <%= order.currentStatus %>
                            </td>
                            <td>
                                <%= order.assignedTo %>
                            </td>
                            <td>
                                <%= order.jobDate %>
                            </td>
                            <td>
                                <%= order.attempt %>
                            </td>
                            <td>
                                <%= moment(order.lastUpdateDateTime).utcOffset(8).format('DD-MM-YYYY h:mm a') %>
                            </td>
                            <td>
                                <%= order.lastUpdatedBy %>
                            </td>
                            <td>
                                <%= order.dateTimeSubmission 
                                    ? order.dateTimeSubmission 
                                    : (() => {
                                        const dateStr = order.creationDate;
                            
                                        // Check if the date is in YYYY-MM-DD format
                                        const isDateOnly = moment(dateStr, "YYYY-MM-DD", true).isValid();
                            
                                        if (isDateOnly) {
                                            // Return date in DD-MM-YYYY format without adding any time
                                            return moment(dateStr, "YYYY-MM-DD").format("DD-MM-YYYY");
                                        } else {
                                            // Convert any other format to DD-MM-YYYY hh:mm A (keep actual time)
                                            return moment(dateStr).format("DD-MM-YYYY hh:mm A");
                                        }
                                    })()
                                %>
                            </td>
                            <td>
                                <%= order.receiverPhoneNumber %>
                            </td>
                            <td>
                                <%= order.additionalPhoneNumber %>
                            </td>
                            <td>
                                <%= order.receiverAddress %>
                            </td>
                            <td>
                                <%= order.area %>
                            </td>
                            <td>
                                <%= order.items[0].description %>
                            </td>
                            <td>
                                <%= order.paymentMethod %>
                            </td>
                            <td>
                                <%= order.paymentAmount %>
                            </td>
                            <td>
                                <%= order.remarks %>
                            </td>
                        </tr>
                        <% }); %>
                </tbody>
            </table>
        </div>
    </main>

