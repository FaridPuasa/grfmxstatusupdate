    <main class="container mt-4">
        <h1>Status Update Results</h1>
        <div class="table-responsive">
            <table class="table table-bordered table-striped">
                <thead class="table-primary">
                    <tr>
                        <th class="header">Tracking Number</th>
                        <th class="header">Result</th>
                    </tr>
                </thead>
                <tbody>
                    <% processingResults.forEach(result=> { %>
                        <tr>
                            <td class="consignment-number">
                                <%= result.consignmentID %>
                            </td>
                            <td class="no-select">
                                <%= result.status %>
                            </td>
                        </tr>
                        <% }); %>
                </tbody>
            </table>
        </div>

        <button class="btn btn-primary" onclick="window.location.href = '/'">Back to Update Delivery</button>
    </main>

    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <script src="https://unpkg.com/bootstrap@5.0.0-alpha1/dist/js/bootstrap.bundle.min.js"></script>
    <script
        src="https://cdnjs.cloudflare.com/ajax/libs/jquery.tablesorter/2.31.0/js/jquery.tablesorter.min.js"></script>
    <script>
        $(document).ready(function () {
            $("table").tablesorter();
        });
    </script>
</body>

</html>