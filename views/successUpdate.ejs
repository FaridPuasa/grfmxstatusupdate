<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Update Success</title>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/5.0.0-alpha1/css/bootstrap.min.css">
    <link rel="stylesheet"
        href="https://cdnjs.cloudflare.com/ajax/libs/jquery.tablesorter/2.31.0/css/theme.default.min.css">
    <style>
        /* Custom CSS for your page */
        .navbar-nav.flex-row {
            flex-direction: row !important;
        }

        /* Make the navbar background transparent */
        .navbar {
            background-color: transparent;
        }

        /* Add background color to the container */
        .container-bg {
            background-color: #333;
            /* You can use your desired background color here */
        }

        /* Add margin to the navigation links */
        .navbar-nav.flex-row .nav-item {
            margin-right: 10px;
        }

        /* Add margin to the submit button */
        .btn-submit {
            margin-top: 10px;
            /* Adjust the margin-top as needed */
        }

        th {
            background-color: #f2f2f2;
        }

        .logo {
  height: 40px;
  width: auto;
  max-height: 40px;
}

@media (max-width: 768px) {
  .logo {
    height: 30px;
    max-height: 30px;
  }
}

@media (max-width: 480px) {
  .logo {
    height: 25px;
    max-height: 25px;
  }
}
    </style>

    <!-- Favicon -->
  <link rel="icon" type="image/png" href="/images/smallgorushlogo.png">
  <link rel="icon" href="/images/smallgorushlogo.png" type="image/x-icon">
<link rel="icon" href="/images/smallgorushlogo.png" type="image/png">
<link rel="apple-touch-icon" href="/images/favicon.png">
</head>

<body class="bg-light">
    <div class="container-bg">
        <div class="container">
            <div class="row">
                <div class="col-12">
                    <%- include('partials/navbar') %>
                </div>
            </div>
        </div>
    </div>

    <main class="container mt-4">
        <h1 class="text-center mt-4">Status Update Results</h1>
        <div class="table-responsive">
            <table class="table table-bordered table-striped">
                <thead class="table-primary">
                    <tr>
                        <th class="header">Tracking Number</th>
                        <th class="header">Result</th>
                    </tr>
                </thead>
                <tbody>
                    <% processingResults.forEach(result=> { %>
                        <tr>
                            <td class="consignment-number">
                                <%= result.consignmentID %>
                            </td>
                            <td class="no-select">
                                <%= result.status %>
                            </td>
                        </tr>
                        <% }); %>
                </tbody>
            </table>
        </div>

        <button class="btn btn-primary" onclick="window.location.href = '/updateDelivery'">Back to Update Delivery</button>
    </main>

    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <script src="https://unpkg.com/bootstrap@5.0.0-alpha1/dist/js/bootstrap.bundle.min.js"></script>
    <script
        src="https://cdnjs.cloudflare.com/ajax/libs/jquery.tablesorter/2.31.0/js/jquery.tablesorter.min.js"></script>
    <script>
        $(document).ready(function () {
            $("table").tablesorter();
        });
    </script>
</body>

</html>