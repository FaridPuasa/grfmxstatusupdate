                        <th>Customer Address</th>
                        <th data-excel-ignore>Area</th>
                    </tr>
                </thead>
                <tbody>
                    <% for (const entry of entries) { %>
                        <tr>
                            <td>
                                <%= entry.customerAddress %>
                            </td>
                            <td>
                                <%= entry.area %>
                            </td>
                        </tr>
                        <% } %>
                </tbody>
            </table>
        </div>
        <button id="copyToExcelBtn" class="btn btn-success">Copy Area to Excel</button>
