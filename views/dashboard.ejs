            <% if (Object.keys(deliveriesMap).length === 0) { %>
                <p class="text-center text-muted">No deliveries found.</p>
            <% } else { %>
                <% Object.keys(deliveriesMap).forEach((date, dateIndex) => { 
                    // Count total orders for this date
