    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <script src="https://unpkg.com/bootstrap@5.0.0-alpha1/dist/js/bootstrap.bundle.min.js"></script>
    <script src="https://cdn.datatables.net/1.11.5/js/jquery.dataTables.min.js"></script>
    <script src="https://cdn.datatables.net/buttons/2.2.2/js/dataTables.buttons.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jszip/3.1.3/jszip.min.js"></script>
    <script src="https://cdn.datatables.net/buttons/2.2.2/js/buttons.html5.min.js"></script>

    <script>
        $(document).ready(function () {
            const table = $('#orderTable').DataTable({
                "ordering": false,
                "iDisplayLength": 100,
                "dom": 'Bfrtip',
                "buttons": [
                    'copy', 'csv', 'excel', 'pdf', 'print'
                ]
            });

            const areaFilters = {
                "B": ["B", "B1", "B2"],
                "G": ["G", "G1", "G2"],
                "JT": ["JT", "JT1", "JT2", "JT3"],
                "KB": ["KB", "KB / SERIA", "LUMUT", "SERIA"],
                "TUT": ["TUTONG"],
                "TEMB": ["TEMBURONG"],
                "N/A": ["N/A", "", null]
            };

            const productFilters = {
                "MOH": "pharmacymoh",
                "JPMC": "pharmacyjpmc",
                "PHC": "pharmacyphc",
                "LD": "localdelivery",
                "CBSL": "cbsl",
                "EWE": "ewe",
                "EWENS": "ewens",
                "FMX": "fmx",
                "ICARUS": "icarus",
                "JOYBEAN": "localdeliveryjb",
                "BAIDURI": "bb",
                "FATHAN": "fcas",
                "GRP": "grp"
            };

            let activeAreaFilter = null;
            let activeProductFilter = null;

            $.fn.dataTable.ext.search.push(function (settings, data, dataIndex) {
                const area = data[13]; // The column index for the 'area' column in the table
                const product = data[1]; // The column index for the 'product' column in the table

                if (activeAreaFilter && !areaFilters[activeAreaFilter].includes(area)) {
                    return false;
                }

                if (activeProductFilter && productFilters[activeProductFilter] !== product) {
                    return false;
                }

                return true;
            });

            $('.filter-btn').click(function () {
                const filter = $(this).data('filter');
                if ($(this).hasClass('active-filter')) {
                    $(this).removeClass('active-filter');
                    activeAreaFilter = null;
                    $('#productFilterButtons').hide();
                } else {
                    $('.filter-btn').removeClass('active-filter');
                    $(this).addClass('active-filter');
                    activeAreaFilter = filter;
                    $('#productFilterButtons').show();
                    updateProductButtons(filter);
                }
                activeProductFilter = null;
                $('.product-filter-btn').removeClass('active-filter');
                table.draw();
            });

            $(document).on('click', '.product-filter-btn', function () {
                if ($(this).hasClass('active-filter')) {
                    $(this).removeClass('active-filter');
                    activeProductFilter = null;
                } else {
                    $('.product-filter-btn').removeClass('active-filter');
                    $(this).addClass('active-filter');
                    activeProductFilter = $(this).data('product');
                }
                table.draw();
            });

            function updateProductButtons(area) {
                const counts = <%- JSON.stringify(productCounts) %> [area];

                $('#productFilterButtons').empty();
                for (const product in counts) {
                    if (counts[product] > 0) {
                        $('#productFilterButtons').append(
                            `<button class="btn btn-secondary product-filter-btn" data-product="${product}">${product} (${counts[product]})</button>`
                        );
                    }
                }
            }

        });
    </script>
</body>

</html>