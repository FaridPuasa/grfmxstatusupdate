        }

        th {
            background-color: #f2f2f2;
            font-size: 15px;
            text-align: center;
            /* Center-align table headers horizontally */
        }

        td {
            font-size: 15px;
            text-align: center;
            /* Center-align table data cells horizontally */
        }

        table {
            width: 80%;
            /* Set the desired width for the table */
            margin: 0 auto;
            /* Center the table horizontally */
            vertical-align: middle;
            /* Center-align table contents vertically */
        }

        .active-filter {
            background-color: #28a745 !important;
            /* Change to the desired color */
            border-color: #28a745 !important;
        }

        /* Container for area filter buttons */
        #filterButtons {
            display: flex;
            justify-content: center;
            flex-wrap: wrap;
            margin-bottom: 20px;
            /* Adjust the margin as needed */
        }

        /* Container for product filter buttons */
        #productFilterButtons {
            display: flex;
            justify-content: center;
            flex-wrap: wrap;
            margin-bottom: 20px;
            /* Adjust the margin as needed */
        }

        /* Style for individual filter buttons */
        .filter-btn,
        .product-filter-btn {
            margin: 5px;
            /* Adjust spacing between buttons */
            padding: 10px 20px;
            /* Adjust padding inside buttons */
        }

        /* Optional: Add some style for active buttons */
        .active-filter {
            background-color: #007bff;
            color: #fff;
            border-color: #007bff;
        }
    </style>
</head>

